{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "null" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "(.*)", 
      "destination": "https://app-formation-tools-app.vercel.app/"
    }
  ],
  "functions": {
    "src/pages/api/hello.js": {
      "runtime": "@vercel/node@1.15.4"
    }
  }
}
